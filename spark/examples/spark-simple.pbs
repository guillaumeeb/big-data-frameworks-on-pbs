#!/bin/bash
#PBS -N spark-wordcount
#PBS -l select=3:ncpus=5:mem=20000Mb
#PBS -l walltime=00:10:00
module load spark

INPUT=$SPARK_HOME/conf/
cd $PBS_O_WORKDIR
